{
    "name": "birkof/rbac-bundle",
    "type": "symfony-bundle",
    "description": "Symfony package to allow usage ot RBAC control access for your project",
    "keywords": [
        "RBAC", "Security", "Access-Control", "Role", "phprbac.net", "owasp", "symfony", "bundle", "security"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Olivier Fouache",
            "email": "olivier.fouache@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "symfony/framework-bundle": "^6.0",
        "webmozart/assert": "^1.10",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.16"
    },
    "autoload": {
        "psr-4": { "PhpRbacBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\PhpRbacBundle\\": "tests/" }
    }
}
